{
  "name": "next-step-events",
  "version": "1.0.0",
  "description": "Next Step Events is a collaboration tool for what happens after a conference, meetup, or any gathering where like minded individuals are trying to connect and capture the details of the event.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run react-dev\" \" npm run server-dev\" \"npm run scss-dev\"",
    "react-dev": "webpack -d -p --watch --config webpack.config.dev.js",
    "server-dev": "nodemon server/index.js",
    "scss-dev": "sass --watch --style compressed client-react/src/preprocessor:client-react/dist/assets/css",
    "build": "webpack -d -p --config webpack.config.prod.js",
    "start": "concurrently \"npm run build\" \"forever start server/index.js\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeanHinNet/next-step-events.git"
  },
  "author": "Dean Hin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeanHinNet/next-step-events/issues"
  },
  "homepage": "https://github.com/DeanHinNet/next-step-events#readme",
  "dependencies": {
    "axios": "0.18.0",
    "bcrypt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.18.3",
    "bulma": "^0.7.1",
    "bulma-start": "0.0.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "express": "4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "express-mysql-session": "^2.0.0",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "forever-monitor": "^1.7.1",
    "moment": "^2.22.2",
    "mysql": "^2.15.0",
    "mysql-server": "^1.0.5",
    "nodemon": "^1.17.5",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-router-dom": "^4.3.1",
    "request": "^2.87.0",
    "uglify-js": "^3.4.4"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "node": "8.11.0",
    "npm": "6.1.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "2.1.4"
  }
}
